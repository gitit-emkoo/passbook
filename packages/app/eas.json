{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_KOTLIN_VERSION": "1.9.25",
        "API_URL": "https://kimssam-backend.fly.dev",
        "PUBLIC_URL": "https://passbook.today"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "development-client": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_KOTLIN_VERSION": "1.9.25",
        "API_URL": "https://kimssam-backend.fly.dev",
        "PUBLIC_URL": "https://passbook.today"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "env": {
        "EXPO_KOTLIN_VERSION": "1.9.25",
        "API_URL": "https://kimssam-backend.fly.dev",
        "PUBLIC_URL": "https://passbook.today"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "developmentClient": false,
      "env": {
        "EXPO_KOTLIN_VERSION": "1.9.25",
        "API_URL": "https://kimssam-backend.fly.dev",
        "PUBLIC_URL": "https://passbook.today"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

